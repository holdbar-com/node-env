{
  "name": "@riddance/env",
  "version": "0.0.11",
  "type": "module",
  "keywords": [
    "riddance",
    "environment",
    "devenv",
    "opinionated"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/node": "^16",
    "@types/mocha": "10.0.1",
    "@tsconfig/node16": "1.0.3",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "@typescript-eslint/parser": "5.45.1",
    "cspell-lib": "6.17.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "45.0.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "mocha": "10.1.0",
    "prettier": "2.8.1",
    "source-map-support": "0.5.21",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@types/eslint": "8.4.10",
    "@types/prettier": "2.7.1"
  },
  "scripts": {
    "watch": "tsc && node ./bin/prepare.js && node ./bin/install.js && node ./bin/watch.js",
    "prepare": "tsc && eslint && node ./bin/prepare.js",
    "postinstall": "tsc && node ./bin/install.js"
  },
  "files": [
    "template/**",
    "bin/*.js",
    "bin/lib/*.js"
  ],
  "bin": {
    "riddance-watch": "./bin/watch.js"
  }
}
