{
  "name": "@riddance/env",
  "version": "0.0.11",
  "type": "module",
  "keywords": [
    "riddance",
    "environment",
    "devenv",
    "opinionated"
  ],
  "license": "MIT",
  "dependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/mocha": "10.0.6",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "cspell-lib": "8.3.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "50.0.1",
    "mocha": "10.2.0",
    "prettier": "3.2.5",
    "source-map-support": "0.5.21",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@types/eslint": "8.4.10",
    "@types/prettier": "2.7.1"
  },
  "scripts": {
    "watch": "tsc && node ./bin/prepare.js && node ./bin/install.js && node ./bin/watch.js",
    "prepare": "tsc && eslint && node ./bin/prepare.js",
    "postinstall": "node ./bin/install.js"
  },
  "files": [
    "template/**",
    "bin/*.js",
    "bin/lib/*.js"
  ],
  "bin": {
    "riddance-watch": "./bin/watch.js"
  }
}
